{
  "custom": {},
  "params": {
    "instances": [
      {
        "destination": "/",
        "icon": "novotek_icons/todo-line",
        "label": "items"
      },
      {
        "destination": "/",
        "icon": "novotek_icons/todo-line",
        "label": "items"
      }
    ],
    "isOpen": true,
    "name": "Section A"
  },
  "propConfig": {
    "params.instances": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.isOpen": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.name": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 200,
      "width": 230
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "21px",
          "shrink": 0
        },
        "propConfig": {
          "meta.visible": {
            "binding": {
              "config": {
                "path": "view.params.isOpen"
              },
              "type": "property"
            }
          },
          "position.display": {
            "binding": {
              "config": {
                "expression": "len({view.params.name}) \u003e 0"
              },
              "type": "expr"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.name"
              },
              "transforms": [
                {
                  "code": "\treturn value",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "colors/text/grey500 typo/text_sm typo/weight/regular",
            "whiteSpace": "nowrap"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "FlexRepeater"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "props.instances": {
            "binding": {
              "config": {
                "path": "view.params.instances"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "direction": "column",
          "elementPosition": {
            "basis": "48px",
            "grow": 0,
            "shrink": 0
          },
          "path": "Components/Menu/NavItem",
          "style": {
            "gap": "8px",
            "overflow": "hidden"
          },
          "useDefaultViewHeight": false,
          "useDefaultViewWidth": false
        },
        "type": "ia.display.flex-repeater"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "gap": "8px",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}