{
  "custom": {},
  "params": {
    "enabled": true,
    "icons": {
      "left": {
        "display": true,
        "value": "novotek_icons/search-2-line"
      }
    },
    "label": {
      "display": true,
      "value": "Label"
    },
    "message": {
      "display": true,
      "value": "Petite aide ou erreur"
    },
    "multiSelect": false,
    "options": [],
    "placeholder": "Selectionner...",
    "value": ""
  },
  "propConfig": {
    "params.enabled": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.icons": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.label": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.message": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.multiSelect": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.options": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.placeholder": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 98
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "view.params.label.display"
              },
              "type": "property"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.label.value"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "typo/text_md typo/weight/bold"
          }
        },
        "type": "ia.display.label"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Dropdown"
            },
            "propConfig": {
              "props.enabled": {
                "binding": {
                  "config": {
                    "expression": "{view.params.enabled}"
                  },
                  "type": "expr"
                }
              },
              "props.multiSelect": {
                "binding": {
                  "config": {
                    "path": "view.params.multiSelect"
                  },
                  "type": "property"
                }
              },
              "props.options": {
                "binding": {
                  "config": {
                    "path": "view.params.options"
                  },
                  "type": "property"
                }
              },
              "props.placeholder.text": {
                "binding": {
                  "config": {
                    "path": "view.params.placeholder"
                  },
                  "type": "property"
                }
              },
              "props.style.paddingLeft": {
                "binding": {
                  "config": {
                    "expression": "if({view.params.icons.left.display}, \"32px\", \"12px\")"
                  },
                  "type": "expr"
                }
              },
              "props.value": {
                "binding": {
                  "config": {
                    "bidirectional": true,
                    "path": "view.params.value"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "dropdownOptionStyle": {
                "classes": "inputs/dropdown_item typo/text_md typo/weight/regular"
              },
              "placeholder": {
                "color": "var(--grey-500)"
              },
              "style": {
                "classes": "inputs/input_text"
              }
            },
            "type": "ia.input.dropdown"
          },
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tif self.view.params.value !\u003d \"\":\n\t\tself.view.params.value \u003d \"\""
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Icon_left"
            },
            "position": {
              "basis": "20px"
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "path": "view.params.icons.left.display"
                  },
                  "type": "property"
                }
              },
              "props.path": {
                "binding": {
                  "config": {
                    "path": "../Dropdown.props.value"
                  },
                  "transforms": [
                    {
                      "code": "\tif self.getSibling(\"Dropdown\").props.value in [\"\", [], None]:\n\t\treturn self.view.params.icons.left.value\n\telse:\n\t\treturn \"novotek_icons/close-line\"",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              },
              "props.style.classes": {
                "binding": {
                  "config": {
                    "path": "../Dropdown.props.value"
                  },
                  "transforms": [
                    {
                      "code": "\tif self.getSibling(\"Dropdown\").props.value in [\"\", [], None]:\n\t\treturn \"colors/text/grey400\"\n\telse:\n\t\treturn \"buttons/error_blank\"",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "left": "12px",
                "position": "absolute",
                "top": "12px",
                "width": "20px"
              }
            },
            "type": "ia.display.icon"
          }
        ],
        "meta": {
          "name": "section_input"
        },
        "position": {
          "basis": "45px"
        },
        "props": {
          "direction": "column",
          "style": {
            "marginTop": "4px",
            "padding": "2px",
            "position": "relative"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "meta": {
          "name": "Description"
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "view.params.message.display"
              },
              "type": "property"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.message.value"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "typo/text_sm typo/weight/regular colors/text/grey500",
            "marginTop": "4px"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}