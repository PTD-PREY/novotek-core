{
  "custom": {},
  "params": {
    "displayLimitation": true,
    "height": "100%",
    "limitation": "",
    "max": "",
    "min": "",
    "unit": "MW",
    "value": ""
  },
  "propConfig": {
    "params.displayLimitation": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.height": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.limitation": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.max": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.min": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.unit": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 40,
      "width": 227
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Production"
            },
            "position": {
              "grow": 1
            },
            "propConfig": {
              "props.max": {
                "binding": {
                  "config": {
                    "bidirectional": true,
                    "path": "view.params.max"
                  },
                  "overlayOptOut": true,
                  "type": "property"
                }
              },
              "props.min": {
                "binding": {
                  "config": {
                    "bidirectional": true,
                    "path": "view.params.min"
                  },
                  "type": "property"
                }
              },
              "props.style.height": {
                "binding": {
                  "config": {
                    "bidirectional": true,
                    "path": "view.params.height"
                  },
                  "type": "property"
                }
              },
              "props.value": {
                "binding": {
                  "config": {
                    "bidirectional": true,
                    "path": "view.params.value"
                  },
                  "overlayOptOut": true,
                  "type": "property"
                }
              }
            },
            "props": {
              "bar": {
                "color": "--primary-500"
              },
              "style": {
                "zIndex": 10
              },
              "track": {
                "color": "--grey-50"
              },
              "valueDisplay": {
                "format": "none"
              }
            },
            "type": "ia.display.progress"
          },
          {
            "meta": {
              "name": "limitation"
            },
            "position": {
              "basis": "2px",
              "shrink": 0
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "path": "view.params.displayLimitation"
                  },
                  "type": "property"
                },
                "persistent": true
              },
              "props.style.left": {
                "binding": {
                  "config": {
                    "expression": "concat(({view.params.limitation}*98)/{view.params.max}*1.02, \"%\")"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "direction": "column",
              "style": {
                "classes": "colors/back/warning500",
                "height": "100%",
                "padding": "0 2px 0 2px",
                "position": "absolute",
                "width": "2px",
                "zIndex": 99
              }
            },
            "type": "ia.container.flex"
          }
        ],
        "meta": {
          "name": "JaugeSection"
        },
        "position": {
          "basis": "16px",
          "shrink": 0
        },
        "props": {
          "alignItems": "center",
          "style": {
            "overflow": "hidden",
            "padding": "4px",
            "position": "relative"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Limitation"
            },
            "position": {
              "grow": 1,
              "shrink": 0
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "path": "view.params.displayLimitation"
                  },
                  "overlayOptOut": true,
                  "type": "property"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "concat({view.params.limitation} , {view.params.unit})"
                  },
                  "overlayOptOut": true,
                  "type": "expr"
                }
              }
            },
            "props": {
              "style": {
                "classes": "typo/text_sm typo/weight/regular colors/text/warning500"
              }
            },
            "type": "ia.display.label"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "Value"
                },
                "position": {
                  "shrink": 0
                },
                "propConfig": {
                  "props.text": {
                    "binding": {
                      "config": {
                        "path": "view.params.value"
                      },
                      "overlayOptOut": true,
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "style": {
                    "classes": "typo/text_md typo/weight/bold colors/text/primary500"
                  }
                },
                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "Max"
                },
                "position": {
                  "shrink": 0
                },
                "propConfig": {
                  "props.text": {
                    "binding": {
                      "config": {
                        "expression": "if({view.params.max} \u003d \"\" || {view.params.max} \u003d None, {view.params.unit}, concat(\"/\", {view.params.max} , {view.params.unit}))"
                      },
                      "overlayOptOut": true,
                      "type": "expr"
                    }
                  }
                },
                "props": {
                  "style": {
                    "classes": "typo/text_sm typo/weight/regular colors/text/grey500"
                  }
                },
                "type": "ia.display.label"
              }
            ],
            "meta": {
              "name": "Production"
            },
            "position": {
              "shrink": 0
            },
            "props": {
              "style": {
                "gap": "1px"
              }
            },
            "type": "ia.container.flex"
          }
        ],
        "meta": {
          "name": "TextSection"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "justify": "flex-end",
          "style": {
            "padding": "0 4px"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "justify": "center",
      "style": {
        "gap": "4px"
      }
    },
    "type": "ia.container.flex"
  }
}