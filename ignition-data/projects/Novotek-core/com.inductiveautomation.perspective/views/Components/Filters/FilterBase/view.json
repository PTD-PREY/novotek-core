{
  "custom": {},
  "params": {
    "icon": "",
    "indicator": {
      "display": false,
      "icon": ""
    },
    "isOpen": false,
    "title": ""
  },
  "propConfig": {
    "params.icon": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.indicator": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.isOpen": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.title": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 76,
      "width": 208
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Icon"
            },
            "position": {
              "basis": "24px",
              "shrink": 0
            },
            "propConfig": {
              "props.path": {
                "binding": {
                  "config": {
                    "path": "view.params.icon"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": ""
              }
            },
            "type": "ia.display.icon"
          },
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.title"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "typo/text/md typo/weight/bold"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "TitleSection"
        },
        "position": {
          "grow": 1
        },
        "props": {
          "alignItems": "center",
          "style": {
            "gap": "8px",
            "overflow": "hidden"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Indicator"
            },
            "position": {
              "basis": "24px",
              "display": false,
              "shrink": 0
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "path": "view.params.indicator.display"
                  },
                  "type": "property"
                },
                "persistent": true
              },
              "props.params.icons.left.value": {
                "binding": {
                  "config": {
                    "path": "view.params.indicator.icon"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "params": {
                "icons": {
                  "left": {
                    "display": true
                  },
                  "right": {
                    "display": false,
                    "value": "novotek_icons/arrow-right-circle-line"
                  }
                },
                "label": {
                  "display": false,
                  "text": ""
                }
              },
              "path": "Components/Badge",
              "style": {
                "classes": "badges/secondary_fill badges/shape/bdg_rounded \nbadges/size/bdg_icn_sm"
              }
            },
            "type": "ia.display.view"
          },
          {
            "meta": {
              "name": "Icon"
            },
            "position": {
              "basis": "24px",
              "shrink": 0
            },
            "propConfig": {
              "props.path": {
                "binding": {
                  "config": {
                    "path": "view.params.isOpen"
                  },
                  "transforms": [
                    {
                      "fallback": "novotek_icons/arrow-down-s-line",
                      "inputType": "scalar",
                      "mappings": [
                        {
                          "input": true,
                          "output": "novotek_icons/arrow-up-s-line"
                        }
                      ],
                      "outputType": "scalar",
                      "type": "map"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": ""
              }
            },
            "type": "ia.display.icon"
          }
        ],
        "meta": {
          "name": "IndicatorSection"
        },
        "position": {
          "basis": "56px",
          "shrink": 0
        },
        "props": {
          "alignItems": "center",
          "justify": "flex-end",
          "style": {
            "gap": "8px",
            "overflow": "hidden"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "alignItems": "center",
      "style": {
        "classes": "container/filter_base",
        "gap": "24px",
        "padding": "16px 16px"
      }
    },
    "type": "ia.container.flex"
  }
}