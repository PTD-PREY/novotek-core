                "message": {
                  "display": false,
                  "value": ""
                },
                "multiSelect": true,
                "options": [],
                "placeholder": "Select status...",
                "value": ""
              },
              "path": "Components/Inputs/Dropdown"
            },
            "type": "ia.display.view"
          },
          {
            "meta": {
              "name": "EmbeddedView_1"
            },
            "position": {
              "grow": 1
            },
            "props": {
              "params": {
                "icons": {
                  "left": {
                    "display": false,
                    "value": ""
                  },
                  "right": {
                    "display": false,
                    "value": ""
                  }
                },
                "label": {
                  "display": true,
                  "value": "Area"
                },
                "message": {
                  "display": false,
                  "value": ""
                },
                "multiSelect": true,
                "options": [],
                "placeholder": "Select status...",
                "value": ""
              },
              "path": "Components/Inputs/Dropdown"
            },
            "type": "ia.display.view"
          }
        ],
        "meta": {
          "name": "filtersSection"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "parent.custom.isOpen"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "alignItems": "flex-start",
          "style": {
            "classes": "borders/borderBottom colors/border/grey200",
