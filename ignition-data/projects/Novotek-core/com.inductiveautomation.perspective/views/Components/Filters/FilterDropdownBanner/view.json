        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "EmbeddedView"
            },
            "position": {
              "basis": "260px",
              "shrink": 0
            },
            "props": {
              "params": {
                "enabled": true,
                "icons": {
                  "left": {
                    "display": true,
