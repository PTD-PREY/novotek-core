{
  "custom": {},
  "params": {
    "selected": [
      "Category 1"
    ],
    "unselected": [
      "Category 2",
      "Category 3"
    ]
  },
  "propConfig": {
    "params.selected": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.unselected": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 400,
      "width": 500
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Title"
            },
            "position": {
              "shrink": 0
            },
            "props": {
              "style": {
                "classes": "typo/text_sm typo/weight/regular colors/text/grey500"
              },
              "text": "COMMON_SELECTED"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "SelectedRepeater"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.instances": {
                "binding": {
                  "config": {
                    "path": "view.params.selected"
                  },
                  "transforms": [
                    {
                      "code": "\tselectedBind \u003d []\n\t\n\tfor selected in value:\n\t\telem \u003d {\n\t\t  \"label\": selected,\n\t\t  \"selected\": True\n\t\t}\n\t\t\n\t\tselectedBind.append(elem)\n\t\t\n\treturn sorted(selectedBind)",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "alignContent": "flex-start",
              "alignItems": "flex-start",
              "elementPosition": {
                "basis": "auto",
                "grow": 0,
                "shrink": 0
              },
              "path": "Components/Filters/Panel/Category",
              "style": {
                "gap": "16px"
              },
              "useDefaultViewHeight": false,
              "useDefaultViewWidth": false,
              "wrap": "wrap"
            },
            "type": "ia.display.flex-repeater"
          }
        ],
        "meta": {
          "name": "SelectedSection"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "direction": "column",
          "style": {
            "gap": "8px"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Title"
            },
            "position": {
              "shrink": 0
            },
            "props": {
              "style": {
                "classes": "typo/text_sm typo/weight/regular colors/text/grey500"
              },
              "text": "COMMON_UNSELECTED"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "UnselectedRepeater"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.instances": {
                "binding": {
                  "config": {
                    "path": "view.params.unselected"
                  },
                  "transforms": [
                    {
                      "code": "\tunselectedBind \u003d []\n\t\n\tfor unselected in value:\n\t\telem \u003d {\n\t\t  \"label\": unselected,\n\t\t  \"selected\": False\n\t\t}\n\t\t\n\t\tunselectedBind.append(elem)\n\t\t\n\treturn sorted(unselectedBind)",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "alignContent": "flex-start",
              "alignItems": "flex-start",
              "elementPosition": {
                "basis": "auto",
                "grow": 0,
                "shrink": 0
              },
              "path": "Components/Filters/Panel/Category",
              "style": {
                "gap": "16px"
              },
              "useDefaultViewHeight": false,
              "useDefaultViewWidth": false,
              "wrap": "wrap"
            },
            "type": "ia.display.flex-repeater"
          }
        ],
        "meta": {
          "name": "UnselectedSection"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "direction": "column",
          "style": {
            "gap": "8px"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tpayloadSend \u003d {\n\t\t\u0027selected\u0027: False,\n\t\t\u0027label\u0027: \"selectAll\"\n\t}\n\t\n\tsystem.perspective.sendMessage(\"FilterCategoryClicked\", payload\u003dpayloadSend)"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "SelectAllButton"
            },
            "position": {
              "grow": 1
            },
            "props": {
              "image": {
                "height": 20,
                "icon": {
                  "color": "var(--primary-500)",
                  "path": "novotek_icons/check-double-fill"
                },
                "width": 20
              },
              "style": {
                "classes": "buttons/primary_blank buttons/size/btn_txt_md"
              },
              "text": "COMMON_SELECT_ALL"
            },
            "type": "ia.input.button"
          },
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tpayloadSend \u003d {\n\t\t\u0027selected\u0027: True,\n\t\t\u0027label\u0027: \"unselectAll\"\n\t}\n\t\t\n\tsystem.perspective.sendMessage(\"FilterCategoryClicked\", payload\u003dpayloadSend)"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "UnselectAllButton"
            },
            "position": {
              "grow": 1
            },
            "props": {
              "image": {
                "height": 20,
                "icon": {
                  "color": "var(--primary-500)",
                  "path": "novotek_icons/close-fill"
                },
                "width": 20
              },
              "style": {
                "classes": "buttons/primary_blank buttons/size/btn_txt_md"
              },
              "text": "COMMON_UNSELECT_ALL"
            },
            "type": "ia.input.button"
          }
        ],
        "meta": {
          "name": "ButtonsSection"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "style": {
            "classes": "borders/borderTop colors/border/grey100",
            "gap": "16px",
            "paddingTop": "16px"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "borderRadius": "4px",
        "classes": "colors/back/background borders/borderAll colors/border/grey200 effects/shadows_md",
        "gap": "32px",
        "height": "fit-content",
        "margin": "8px",
        "padding": "16px 24px"
      }
    },
    "type": "ia.container.flex"
  }
}