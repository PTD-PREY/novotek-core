{
  "custom": {},
  "params": {
    "icons": {
      "left": {
        "display": true,
        "value": "novotek_icons/timer-line"
      },
      "right": {
        "display": true,
        "value": "novotek_icons/arrow-right-circle-line"
      }
    },
    "label": {
      "display": true,
      "text": "Badge"
    }
  },
  "propConfig": {
    "params.icons": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.label": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 36,
      "width": 173
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "16px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "view.params.icons.left.display"
              },
              "overlayOptOut": true,
              "type": "property"
            }
          },
          "props.path": {
            "binding": {
              "config": {
                "path": "view.params.icons.left.value"
              },
              "overlayOptOut": true,
              "type": "property"
            },
            "persistent": false
          }
        },
        "props": {
          "style": {
            "minHeight": "16px"
          }
        },
        "type": "ia.display.icon"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "view.params.label.display"
              },
              "overlayOptOut": true,
              "type": "property"
            }
          },
          "props.style.marginLeft": {
            "binding": {
              "config": {
                "path": "view.params.icons.left.display"
              },
              "transforms": [
                {
                  "code": "\treturn \"4px\" if value \u003d\u003d True else \"0px\"",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.style.marginRight": {
            "binding": {
              "config": {
                "path": "view.params.icons.right.display"
              },
              "transforms": [
                {
                  "code": "\treturn \"4px\" if value \u003d\u003d True else \"0px\"",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.label.text"
              },
              "overlayOptOut": true,
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "whiteSpace": "nowrap"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Icon_0"
        },
        "position": {
          "basis": "16px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "view.params.icons.right.display"
              },
              "overlayOptOut": true,
              "type": "property"
            }
          },
          "props.path": {
            "binding": {
              "config": {
                "path": "view.params.icons.right.value"
              },
              "overlayOptOut": true,
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "marginRight": 5,
            "minHeight": "16px"
          }
        },
        "type": "ia.display.icon"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "alignItems": "center",
      "justify": "center",
      "style": {
        "overflow": "hidden",
        "padding": "0 8px",
        "width": "fit-content"
      }
    },
    "type": "ia.container.flex"
  }
}